{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "MLP",
    "dataset": "MNIST",
    
    "out_dir": "out/superpixel_graph_classification/",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 128,
        "init_lr": 1e-3,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 168,
        "out_dim": 168,
        "graph_pool": "mean",
        "gated": false,
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "density": 0.1,
        "activation": "relu",
        "mlp_layers": 2,
        "sampler": "regular",
        "linear_type": "regular",
        "bias": true,
        "dgl_builtin": false
    }
}