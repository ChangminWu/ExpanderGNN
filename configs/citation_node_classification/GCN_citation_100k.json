{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "GCN",
    "dataset": "cora",    
    "out_dir": "out/citation_node_classification/",

    "params": {
        "seed": 41,
        "num_split": 10,
        "epochs": 200,
        "batch_size": 128,
        "init_lr": 0.01,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 200,
        "min_lr": 1e-5,
        "weight_decay": 5e-5,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 3,
        "hidden_dim": 256,
        "out_dim": 256,
        "residual": true,
        "graph_pool": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "self_loop": false,
        "density": 0.1,
        "activation": "relu",
        "neighbor_pool": "sum",
        "mlp_layers": 2,
        "sampler": "regular",
        "linear_type": "regular",
        "bias": true,
        "dgl_builtin": false
    }
}